<!-- navbar -->
<header class="navbar a a-fi-down a-del-500" id="js-navbar">
    <a href="/">
        <span class="opacity-hover js-logo">
            <svg class="logo" xmlns="http://www.w3.org/2000/svg" width="69" height="77" viewBox="0 0 69 77"><path d="M69,77H0V29H7.1L8.016,0H21.979L22,.479,23.225,29h1.723L26.026,9H39.974l1.079,20H69ZM1,76H68V30H40.105L39.026,10H26.974L25.895,30H22.268l-.021-.479L21.021,1H8.984L8.067,30H1Z"/><path d="M10.5,42A3.5,3.5,0,1,1,14,38.5,3.5,3.5,0,0,1,10.5,42Zm0-6A2.5,2.5,0,1,0,13,38.5,2.5,2.5,0,0,0,10.5,36Z"/><path d="M22.5,42A3.5,3.5,0,1,1,26,38.5,3.5,3.5,0,0,1,22.5,42Zm0-6A2.5,2.5,0,1,0,25,38.5,2.5,2.5,0,0,0,22.5,36Z"/><path d="M34.5,42A3.5,3.5,0,1,1,38,38.5,3.5,3.5,0,0,1,34.5,42Zm0-6A2.5,2.5,0,1,0,37,38.5,2.5,2.5,0,0,0,34.5,36Z"/><path d="M46.5,42A3.5,3.5,0,1,1,50,38.5,3.5,3.5,0,0,1,46.5,42Zm0-6A2.5,2.5,0,1,0,49,38.5,2.5,2.5,0,0,0,46.5,36Z"/><path d="M58.5,42A3.5,3.5,0,1,1,62,38.5,3.5,3.5,0,0,1,58.5,42Zm0-6A2.5,2.5,0,1,0,61,38.5,2.5,2.5,0,0,0,58.5,36Z"/></svg>
        </span>
    </a>
    <ul class="navbar__list">
        {{#each navbar}}
        <span class="opacity-hover">
            <li class="navbar__list-item navbar__list-item--{{item}} js-nav js-nav--{{item}}">
                {{item}}
            </li>
        </span>
        {{/each}}
    </ul>
</header>

<!-- animation of lines -->
<div class="lines">
    <div class="lines__line js-line"><span></span></div>
    <div class="lines__line js-line"><span></span></div>
    <div class="lines__line js-line"><span></span></div>
    <div class="lines__line js-line"><span></span></div>
    <div class="lines__line js-line"><span></span></div>
</div>

<!-- animation of little objects -->
<div class="ao js-ao">
    <div class="ao__particle ao--move-up-down ao--v-70"></div>
    <div class="ao__particle ao--move-up-down ao--v-20 ao--delay-2s"></div>
    <div class="ao__particle ao--move-up-down ao--v-22 ao--delay-3s ao--dur-11s"></div>
    <div class="ao__particle ao--move-up-down ao--v-30 ao--delay-5s"></div>
    <div class="ao__particle ao--move-up-down ao--v-50 ao--delay-7s ao--dur-10s"></div>
    <div class="ao__particle ao--move-up-down ao--v-80 ao--delay-6s ao--dur-13s"></div>
</div>

<!-- pages -->
{{#each pages}}
<div id="{{page}}" class="js-page js-page--{{page}}"></div>
{{/each}}