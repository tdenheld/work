	<a href="/">
		<span include="../assets/img/logo-tk305.svg" class="opacity-hover"></span>
	</a>
	<ul class="navbar__list">
		{{#each navbar}}
			<li class="navbar__list-item navbar__list-item--{{item}} js-nav js-nav--{{item}} opacity-hover">
